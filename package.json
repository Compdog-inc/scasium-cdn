{
    "name": "scasium-cdn",
    "version": "0.1.0",
    "description": "CDN for Scasium",
    "private": true,
    "scripts": {
        "clean": "rm -rf build node_modules",
        "build": "eslint . && tsc",
        "start": "vercel dev",
        "dev": "nodemon --ext ts --watch src --exec ./node_modules/.bin/ts-node ./src/server.ts",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix"
    },
    "dependencies": {
        "body-parser": "^1.20.0",
        "debug": "^4.3.4",
        "express": "^4.18.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "eslint": "^8.20.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4",
        "vercel": "^19.1.2"
    }
}
